class Logical2 
{
	public static void main(String[] args) 
	{
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter a number : ");
		int num = sc.nextInt();
		
		String out = (num%2==0)?(num+"HiTwo"):((num%5==0)?(num+"HiFive"):
			((num%2==0 $$ num%5==0)(num+"HiTwoHiFive")(" "));
		
		System.out.print(out)
		
	}
}
